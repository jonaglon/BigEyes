bool getEightPointStarAnimMask(byte number, byte row){
 return (bool)pgm_read_byte(&eightPointStarAnimMask[number][row]);
}

bool getSnowflakeAnimationMask1(byte number, byte row){
 return (bool)pgm_read_byte(&snowflakeAnimationMask1[number][row]);
}

bool getSnowflakeAnimationMask2(byte number, byte row){
 return (bool)pgm_read_byte(&snowflakeAnimationMask2[number][row]);
}


void doStar() {
  int animationStep1 = (totalTimey / 256)%12;

  for(int led = 0; led < 181; led++) {
    if (getEightPointStarAnimMask(animationStep1,led)) {
      int x = quickAbsolute(100-eyeCoords[led][0]);
      int y = quickAbsolute(105-eyeCoords[led][1]);
      SetRgbwWheelVars(((timey/64)-x-y)%256);
      setLedDirect(led, wheelR, wheelG, wheelB, 0, false);    
      //setPixel(led, 100, 100, 100, 0);
    }
  }
}

void doSnowflake() {
  // 16384 is a beat length  1365 is an 12th-ish
  //int animationStep = percentThroughBeat / 1365;
  
  int animationStep1 = (totalTimey / 128)%16;
  int animationStep2 = (totalTimey / 192)%12;
  
  for(int led = 0; led < 181; led++) {
    // setPixel(led, 0, 0, 0, 0);
    if (getSnowflakeAnimationMask1(animationStep1,led)) {
      for (int j = 1; j < 10; j++) { 
        int ledToLightFrom = circleFirstLeds[j-1];
        int ledToLightTo = circleFirstLeds[j];
        SetRgbwWheelVarsBlueToWhite(((totalTimey/16)+(j*20))%256);
        if (led >= ledToLightFrom && led < ledToLightTo) { 
          setLedDirect(led, wheelR, wheelG, wheelB, 0, false);    
        }
      }
    }
    if (getSnowflakeAnimationMask2(animationStep2,led)) {
      for (int j = 1; j < 10; j++) { 
        int ledToLightFrom = circleFirstLeds[j-1];
        int ledToLightTo = circleFirstLeds[j];
        SetRgbwWheelVarsBlueToWhite(((totalTimey/24)+(j*18))%256);
        if (led >= ledToLightFrom && led < ledToLightTo) { 
          setLedDirect(led, wheelR, wheelG, wheelB, 0, false);    
        }
      }
    }
  }
}
